{
  "name": "ab-calicritters",
  "version": "1.0.0",
  "private": true,
  "description": "AB Calicritters monorepo",
  "workspaces": [
    "packages/*"
  ],
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "npm run dev -w @ab-calicritters/api",
    "build": "npm run build -w @ab-calicritters/api && npm run build -w @ab-calicritters/jobs && npm run build -w @ab-calicritters/dashboard",
    "start": "npm run start -w @ab-calicritters/api",
    "check:determinism": "npm run check:determinism -w @ab-calicritters/api",
    "jobs:rollup": "npm run rollup -w @ab-calicritters/jobs --",
    "jobs:check:idempotency": "npm run check:idempotency -w @ab-calicritters/jobs",
    "loadtest:run": "npm run loadtest:run -w @ab-calicritters/jobs --",
    "loadtest:assert": "npm run loadtest:assert -w @ab-calicritters/jobs --",
    "loadtest:baseline:assignment": "npm run loadtest:run -- --scenario assignment --phase baseline --target http://localhost:3000",
    "loadtest:baseline:events": "npm run loadtest:run -- --scenario events --phase baseline --target http://localhost:3000",
    "loadtest:baseline:mixed": "npm run loadtest:run -- --scenario mixed --phase baseline --target http://localhost:3000",
    "dashboard:dev": "npm run dev -w @ab-calicritters/dashboard",
    "dashboard:build": "npm run build -w @ab-calicritters/dashboard",
    "dashboard:start": "npm run start -w @ab-calicritters/dashboard",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:seed": "prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^6.3.1"
  },
  "devDependencies": {
    "prisma": "^6.3.1",
    "tsx": "^4.19.3"
  }
}
